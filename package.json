{
  "name": "@cashstar/react-timezone",
  "version": "1.0.3",
  "description": "A timezone picker widget in React",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:js",
    "build:js": "babel src/ -d lib/",
    "clean": "rimraf lib",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "test": "npm run lint && npm run test:jest",
    "test:jest": "jest",
    "test:nocache": "jest --no-cache",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "tag": "git tag v$npm_package_version",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:major": "npm --no-git-tag-version version major",
    "preversion": "npm run test",
    "postversion": "git commit package.json -m \"Version $npm_package_version\" && npm run tag && git push && git push --tags && npm publish --registry=https://registry.npmjs.org/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erin-doyle/react-timezone.git"
  },
  "keywords": [
    "timezone"
  ],
  "author": "Carlo DiCelico, Erin Doyle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erin-doyle/react-timezone/issues"
  },
  "homepage": "https://github.com/erin-doyle/react-timezone#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "invariant": "^2.2.2",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "react": "^15.6.1",
    "react-autocomplete": "^1.7.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-knobs": "3.2.8",
    "@storybook/react": "3.2.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^20.0.4",
    "jsdom": "^11.2.0",
    "less": "^2.7.2",
    "prop-types": "^15.5.10",
    "react-a11y-alt": "^0.3.5",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "semver": "^5.4.1"
  }
}
